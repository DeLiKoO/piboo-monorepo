{
  "name": "piboo",
  "version": "1.0.0",
  "description": "PiBoo - Another Photobooth for RaspberryPi",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "scripts": {
    "start": "yarn start:electron",
    "start:web": "env-cmd -f .env.web craco start",
    "start:electron": "electron .",
    "build": "yarn build:electron",
    "build:web": "craco build",
    "prebuild:electron": "electron-rebuild -v 6.1.12 -f sharp && electron-build-env neon build rscam-node-addon --release",
    "build:electron": "env-cmd -f .env.electron craco build",
    "rebuild:electron": "env-cmd -f .env.electron craco build",
    "watch:electron": "npm-watch",
    "test": "craco test",
    "eject": "react-scripts eject",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "watch": {
    "build:electron": "src/**/*.*"
  },
  "author": "Chris Dee",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^1.1.0",
    "bootstrap": "^4.6.0",
    "noderscam": "workspace:^1.0.0",
    "piboo-server": "workspace:^1.0.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.0",
    "react-dom": "^17.0.1",
    "react-redux": "^7.1.3",
    "web-vitals": "^1.1.0",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@craco/craco": "^6.1.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "@tsconfig/node12": "^1.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/ws": "^7.4.0",
    "electron": "6.1.12",
    "electron-build-env": "^0.2.0",
    "electron-builder": "^22.9.1",
    "electron-rebuild": "^2.3.5",
    "env-cmd": "^10.1.0",
    "neon-cli": "^0.7.1",
    "npm-watch": "^0.7.0",
    "react-scripts": "4.0.2",
    "typescript": "~3.7.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
